/div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: [\"products\"],\n  data() {\n    return {\n\n    }\n  },\n  computed: {\n    totalCartPrice() {\n      return this.products.reduce((total, product) => parseFloat(total) + parseFloat(product.price), 0);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.product-container {\n  align-content: \"center\";\n  border-bottom: 1px solid #ddd;\n  display: flex;\n  padding: 16px;\n  width: 100%;\n}\n\n.product-image {\n  flex: 1;\n  height: 100px;\n  max-width: 100px;\n}\n\n.details-wrap {\n  padding: 0 16px;\n  flex: 3;\n}\n</style>"],"mappings":"AA6CA,eAAe;EACbA,KAAK,EAAE,CAAC,UAAU,CAAC;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO,CAEP;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACC,QAAQ,CAACC,MAAM,CAAC,CAACC,KAAK,EAAEC,OAAO,KAAKC,UAAU,CAACF,KAAK,IAAIE,UAAU,CAACD,OAAO,CAACE,KAAK,CAAC,EAAE,CAAC,CAAC;IACnG;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}